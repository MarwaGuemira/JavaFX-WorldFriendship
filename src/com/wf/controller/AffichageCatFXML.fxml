<AnchorPane id="AnchorPane" prefHeight="383.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wf.controller.AffichageCatFXMLController">
